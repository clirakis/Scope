Section "InputDevice" 
     Identifier	"touchscreen" 
     Driver     "evtouch" 
     Option     "Device"     "/dev/input/event1 
     Option     "DeviceName" "touchscreen" 
     Option     "MinX"       "98" 
     Option     "MinY"       "43" 
     Option     "MaxX"       "940"
     Option     "MaxX"       "925" 
     Option     "ReportingMode" "Raw" 
     Option     "Emulate3Buttons" "50" 
     Option     "SendCoreEvents"  "On" 
EndSection
